doctype html
html
  head
    meta(charset='utf-8')
    title 图片转存测试
    script(src='http://lib.sinaapp.com/js/jquery/1.9.1/jquery-1.9.1.min.js')
  body
    input#url(type='text', name='url')
    button#submit Catch
    script.
      $(function(){
        $('#submit').on('click', function(){
          var url = $('#url').val();
          $.ajax({
            url: '/catchImage',
            type: 'get',
            dataType:'jsonp',
            data: {
              url:url
            },
            success:function(data){
              console.log(data);
            }
          });
        });
      });